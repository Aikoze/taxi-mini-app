{
  "name": "taxi-mini-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:alt": "vite --port 5174",
    "build": "CI=false vite build --emptyOutDir",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "vercel --prod",
    "deploy:dev": "vercel",
    "tunnel": "lt --port 5173 --subdomain taxi-mini-app",
    "tunnel:custom": "node start-tunnel.js",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "server:alt": "PORT=3001 node server/index.js",
    "server:dev:alt": "PORT=3001 nodemon server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "dev:all:alt": "concurrently \"npm run dev:alt\" \"npm run server:dev:alt\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/react-query": "^5.67.1",
    "@telegram-apps/sdk-react": "^3.1.2",
    "@twa-dev/sdk": "^6.9.2",
    "@types/lodash": "^4.17.16",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^12.4.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.477.0",
    "node-telegram-bot-api": "^0.66.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.29.0",
    "sonner": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.6.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "localtunnel": "^2.0.2",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
